{
	"definitions": {},
	"$schema": "http://json-schema.org/draft-07/schema#", 
	"$id": "https://example.com/object1624860628.json", 
	"title": "Root", 
	"type": "object",
	"required": [
		"data",
		"total",
		"page",
		"limit",
		"offset"
	],
	"properties": {
		"data": {
			"$id": "#root/data", 
			"title": "Data", 
			"type": "array",
			"default": [],
			"items":{
				"$id": "#root/data/items", 
				"title": "Items", 
				"type": "object",
				"required": [
					"owner",
					"id",
					"image",
					"publishDate",
					"text",
					"tags",
					"link",
					"likes"
				],
				"properties": {
					"owner": {
						"$id": "#root/data/items/owner", 
						"title": "Owner", 
						"type": "object",
						"required": [
							"id",
							"title",
							"firstName",
							"lastName",
							"email",
							"picture"
						],
						"properties": {
							"id": {
								"$id": "#root/data/items/owner/id", 
								"title": "Id", 
								"type": "string",
								"default": "",
								"examples": [
									"60d0fe4f5311236168a109f7"
								],
								"pattern": "^.*$"
							},
							"title": {
								"$id": "#root/data/items/owner/title", 
								"title": "Title", 
								"type": "string",
								"default": "",
								"examples": [
									"mrs"
								],
								"pattern": "^.*$"
							},
							"firstName": {
								"$id": "#root/data/items/owner/firstName", 
								"title": "Firstname", 
								"type": "string",
								"default": "",
								"examples": [
									"Jolanda"
								],
								"pattern": "^.*$"
							},
							"lastName": {
								"$id": "#root/data/items/owner/lastName", 
								"title": "Lastname", 
								"type": "string",
								"default": "",
								"examples": [
									"Lacroix"
								],
								"pattern": "^.*$"
							},
							"email": {
								"$id": "#root/data/items/owner/email", 
								"title": "Email", 
								"type": "string",
								"default": "",
								"examples": [
									"jolanda.lacroix@example.com"
								],
								"pattern": "^.*$"
							},
							"picture": {
								"$id": "#root/data/items/owner/picture", 
								"title": "Picture", 
								"type": "string",
								"default": "",
								"examples": [
									"https://randomuser.me/api/portraits/med/women/32.jpg"
								],
								"pattern": "^.*$"
							}
						}
					}
,
					"id": {
						"$id": "#root/data/items/id", 
						"title": "Id", 
						"type": "string",
						"default": "",
						"examples": [
							"60d21bb367d0d8992e610dc4"
						],
						"pattern": "^.*$"
					},
					"image": {
						"$id": "#root/data/items/image", 
						"title": "Image", 
						"type": "string",
						"default": "",
						"examples": [
							"https://img.dummyapi.io/photo-1532931715567-9c313b12a144.jpg"
						],
						"pattern": "^.*$"
					},
					"publishDate": {
						"$id": "#root/data/items/publishDate", 
						"title": "Publishdate", 
						"type": "string",
						"default": "",
						"examples": [
							"2020-03-11T09:23:31.200Z"
						],
						"pattern": "^.*$"
					},
					"text": {
						"$id": "#root/data/items/text", 
						"title": "Text", 
						"type": "string",
						"default": "",
						"examples": [
							"Patrick Hendry adult black Labrador retriever lying on sand"
						],
						"pattern": "^.*$"
					},
					"tags": {
						"$id": "#root/data/items/tags", 
						"title": "Tags", 
						"type": "array",
						"default": [],
						"items":{
							"$id": "#root/data/items/tags/items", 
							"title": "Items", 
							"type": "string",
							"default": "",
							"examples": [
								"dog"
							],
							"pattern": "^.*$"
						}
					},
					"link": {
						"$id": "#root/data/items/link", 
						"title": "Link", 
						"type": "string",
						"default": "",
						"examples": [
							"http://www.patrickkylehendry.com"
						],
						"pattern": "^.*$"
					},
					"likes": {
						"$id": "#root/data/items/likes", 
						"title": "Likes", 
						"type": "integer",
						"examples": [
							22
						],
						"default": 0
					}
				}
			}

		},
		"total": {
			"$id": "#root/total", 
			"title": "Total", 
			"type": "integer",
			"examples": [
				1
			],
			"default": 0
		},
		"page": {
			"$id": "#root/page", 
			"title": "Page", 
			"type": "integer",
			"examples": [
				0
			],
			"default": 0
		},
		"limit": {
			"$id": "#root/limit", 
			"title": "Limit", 
			"type": "integer",
			"examples": [
				20
			],
			"default": 0
		},
		"offset": {
			"$id": "#root/offset", 
			"title": "Offset", 
			"type": "integer",
			"examples": [
				0
			],
			"default": 0
		}
	}
}
